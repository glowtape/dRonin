<xml>
  <object name="VirtualGyroStatus" settings="false" singleinstance="true">
    <description>The rate gyroscope sensor data, in body frame.</description>
    <access gcs="readwrite" flight="readwrite"/>
    <logging updatemode="manual" period="0"/>
    <telemetrygcs acked="false" updatemode="manual" period="0"/>
    <telemetryflight acked="false" updatemode="throttled" period="1000"/>

    <field name="Gain" defaultvalue="0" elementnames="Roll,Pitch,Yaw" type="float" units="">
      <description/>
    </field>
    <field name="Covariance" defaultvalue="0" elementnames="Roll,Pitch,Yaw" type="float" units="deg/s">
      <description/>
    </field>

  </object>
</xml>
