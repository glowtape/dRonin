<?xml version="1.0"?>
<xml>
	<object name="ActuatorTelemetry" singleinstance="true" settings="false">
		<description>Basic actuator telemetry received via servo channels.</description>
		<field name="Temperature" units="deg C" type="uint8" elements="10" />
		<field name="Voltage" units="V" type="float" elements="10" />
		<field name="Current" units="A" type="float" elements="10" />
		<field name="Consumed" units="mAh" type="uint16" elements="10" />
		<field name="eRPM" units="" type="uint32" elements="10" />
		<access gcs="readwrite" flight="readwrite" />
		<telemetrygcs acked="false" updatemode="manual" period="0" />
		<telemetryflight acked="false" updatemode="throttled" period="1000" />
		<logging updatemode="manual" period="0" />
	</object>
</xml>